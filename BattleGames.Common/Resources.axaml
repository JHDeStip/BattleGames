<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <x:Double x:Key="InitialWindowWidth">1200</x:Double>
    <x:Double x:Key="InitialWindowHeight">600</x:Double>

    <Thickness x:Key="DefaultMargin">7.5</Thickness>
    <Thickness x:Key="ChartItemProgressBarGridMargin">0, 7.5 7.5 7.5</Thickness>

    <CornerRadius x:Key="DefaultCornerRadius">7.5</CornerRadius>
    <CornerRadius x:Key="ChartItemTrackBorderCornerRadius">0 7.5 7.5 0</CornerRadius>

    <x:Double x:Key="BasicDialogWindowMinWidth">250</x:Double>
    <x:Double x:Key="BasicDialogWindowMaxWidth">500</x:Double>
    <Thickness x:Key="BasicDialogMessageTextBlockMargin">0, 5, 5, 5</Thickness>
    <Thickness x:Key="ContainerPadding">5, 0, 0, 0</Thickness>

</ResourceDictionary>
